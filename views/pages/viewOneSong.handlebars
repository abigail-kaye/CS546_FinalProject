<div class="row">
    <div class="col-sm-12">
         <main>
            <h1 class="songName">{{songName}}</h1>
            <h2 class="artist"> Artist: {{artist}}</h2>
            <h2 class="genre"> Genre: {{genre}}</h2>
            <h2 class="rating">Rating: {{avgRating}}</h2>
            <h2>Reviews: </h2>
            <p class ="reviews"> {{reviews}}</p>
        
            <form class="songButtons">
                <button type="button" onclick="alert('Editing a song')"> Edit info</button>
                <button type="button" onclick="alert('Adding a review')">Add review</button>
                <button type="button" onclick="alert('Adding a rating')"> Add rating</button>
                <button type="button" onclick="alert('Adding/Removing to a playlist')"> Add/remove from playlist</button>
                <button type="button" onclick="alert('Permenantly deleting')"> Delete from Library</button>
            </form>
        </main>
    </div>
</div>