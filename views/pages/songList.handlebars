
<div class="row">
    <div class="col-sm-12">
        <h1>Song List:</h1>
        <button type="submit" class="btn btn-primary">Add song</button>
        <button type="submit" class="btn btn-primary">Add playlist</button>
    </div>
    <div>
        <table id="songTable">
            <tr>
                <th> Song Name</th>
                <th> Artist </th>
                <th> Album </th>
                <th> Genre </th>>
                <th> Rating </th>
            </tr>
            {{#each songs}}
            <tr>
                <th>{{this.title}}</th>
                <th>{{this.artist}}</th>
                <th>{{this.album}}</th>
                <th>{{this.genre}}</th>
                <th>{{this.rating}}</th>
            </tr>
            {{/each}}
        </table>
    </div>
    
</div>


